<view class="bar"></view>
<view class="card pa20">
  <view class="vt323 fsheader text-primary mb">{{info.name}}</view>
  <view class="vt323 ftheader text-primary mb40">From {{info.start_date}} to {{info.end_date}}</view>
  <block wx:for="{{progress.weeks}}" wx:key="index">
    <view class="inconsolata fbody text-primary mb">{{item[0]}}</view>
    <view class="inconsolata fbody text-primary mb20">{{item[1]}}</view>  
  </block>
</view>