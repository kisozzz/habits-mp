@import '/stylesheets/index';

.card {
  position: relative;
  width: 660rpx;
  min-height: 200rpx;
  background: #FFFFFF;
  border: 2rpx solid #2C12A1;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.bar{
  width: 100%;
  height: 6rpx;
  background: #2C12A1;
  position: relative;
}

.percent-bar{
  width: 75%
}