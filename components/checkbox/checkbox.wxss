.box-grey{
  position: relative;
  display: flex;
  width: 28.97px;
  height: 28.97px;
  background: #F4F4F4;
  border: 1px solid #18191F;
  border-radius: 50px;
}

.box-red {
  position: relative;
  display: flex;
  width: 28.97px;
  height: 28.97px;
  background: #FFE8F0;
