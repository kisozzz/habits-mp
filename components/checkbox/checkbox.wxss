.box-grey{
  position: relative;
  display: flex;
  width: 60rpx;
  height: 60rpx;
  background: #F4F4F4;
  border: 1px solid #18191F;
  border-radius: 50px;
  justify-content: center;
  align-items: center;
}

.box-red {
  position: relative;
  display: flex;
  width: 60rpx;
  height: 60rpx;
  background: #FFE8F0;
  border: 1px solid #18191F;
  border-radius: 50px;
  justify-content: center;
  align-items: center;
}

.day{
  font-family: VT323;
  font-style: normal;
