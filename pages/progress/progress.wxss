.btn-big {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 340rpx !important;
  height: 80rpx;
  margin-top: 60rpx;
  background: #DBDDFC;
  border: 2rpx solid #2C12A1;
  border-radius: 0 !important;
  box-sizing: border-box;
  box-shadow: 0rpx 6rpx 0rpx #2C12A1;

  font-family: VT323;
  font-style: normal;
  font-weight: normal;
  font-size: 50rpx;
  line-height: 56rpx;
  text-align: center;
  letter-spacing: 3rpx;
  color: #2C12A1;
}

.empty-space {
  height: 200rpx;
}